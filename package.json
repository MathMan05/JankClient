{
	"name": "jankclient",
	"version": "0.2.0",
	"description": "A SpaceBar Client written in TS HTML and CSS to run, clone the repo and do either `npm start` or `bun start` both bun and node are supported, and both should function as expected. To access Jank Client after init simply go to http://localhost:8080/login and login with your username and password.",
	"main": ".dist/index.js",
	"type": "module",
	"scripts": {
		"lint": "npx eslint .",
		"prebuild": "npx rimraf dist",
		"build:compile": "npx swc ./src -d ./dist --strip-leading-paths --copy-files --source-maps",
		"build:extras": "node scripts/build-extras.mjs",
		"build": "npm run build:compile && npm run build:extras",
		"start": "dist/index.js",
		"watch": "scripts/watch.mjs"
	},
	"author": "MathMan05",
	"license": "GPL-3.0",
	"dependencies": {
		"compression": "^1.7.4",
		"express": "^4.19.2",
		"gulp": "^5.0.0",
		"gulp-copy": "^5.0.0",
		"gulp-plumber": "^1.2.1",
		"gulp-sourcemaps": "^3.0.0",
		"gulp-typescript": "^6.0.0-alpha.1"
	},
	"prettier": {
		"useTabs": true,
		"printWidth": 100,
		"semi": true,
		"bracketSpacing": false
	},
	"devDependencies": {
		"@eslint/js": "^9.10.0",
		"@html-eslint/eslint-plugin": "^0.25.0",
		"@html-eslint/parser": "^0.27.0",
		"@stylistic/eslint-plugin": "^2.3.0",
		"@stylistic/eslint-plugin-js": "^2.8.0",
		"@swc/cli": "^0.7.3",
		"@swc/core": "^1.10.1",
		"@types/compression": "^1.7.5",
		"@types/eslint__js": "^8.42.3",
		"@types/express": "^4.17.21",
		"@types/node-fetch": "^2.6.11",
		"@typescript-eslint/eslint-plugin": "^8.14.0",
		"@typescript-eslint/parser": "^8.14.0",
		"eslint": "^8.57.1",
		"eslint-plugin-html": "^8.1.1",
		"eslint-plugin-sonarjs": "^1.0.4",
		"eslint-plugin-unicorn": "^55.0.0",
		"typescript": "^5.6.2",
		"typescript-eslint": "^8.14.0",
		"prettier": "^3.4.2",
		"rimraf": "^6.0.1"
	}
}
